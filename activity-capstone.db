CREATE TABLE IF NOT EXTISTS Orders(
  ord_np TEXT PRIMARY KEY,
  purch_amt REAL,
  ord_date TEXT,
  cutomer_id TEXT,
  Salesman_id TEXT,
  FOREIGN key ( cutomer_id) REFERANCES customer(cutomer_id),
  FOREIGN key ( Salesman_id) REFERANCES Salesman(Salesman_id)
);


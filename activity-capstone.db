CREATE TABEL IF NOT EXISTS Customer(
  customer_id TEXT,
  cust_name TEXT PRIMARY KEY,
  city TEXT,
  grade INTEGER,
  salesman_id TEXT,
  FOREIGN KEY (salesman_id) REFERANCES Salesman(Salesman_id)
);

